<h1> <%= title %></h1>
 
  <ul>
      <% for(var i = 0; i < list_genres.length; i++ ) { %>
            <li>
                <a href= "<%=list_genres[i].url%>"><%=list_genres[i].name %></a>
             </li>
       <% }; %>

  </ul>
<% if(list_genres.length == 0) { %>
  <p>We haven't classified the genres for now. Please check-in later</p>
  <% } %>