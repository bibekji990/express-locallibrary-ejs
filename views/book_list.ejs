<h1> <%= title %> </h1>
  <ul>
      <% for(book in book_list) { %>
          <li>
               <a href="<%=book_list[book].url%>"> <%=book_list[book].title%> </a> (<%=book_list[book].author.name%>)
          </li>
         
       <% }; %>

  </ul>
  
  <% if(book_list.length == 0) { %>
  <p>There are no books currently here. Please check-in later.</p>
  <% } %>
  