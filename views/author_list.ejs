<h1> <%= title %></h1>
  <ul>
      <% for(author in author_list) { %>
           <li>
               <a href="<%=author_list[author].url%>"><%=author_list[author].name %></a> (<%=author_list[author].lifespan%>) 
          </li>
          
     <% };%>

  </ul>
  
 <% if(typeof author_list.length == 0) { %>
  <p>We don't have any author listed at the moment. Please check-in later.</p>
  <% } %>